            <div class="d-grid gap-2 d-sm-flex justify-content-sm-center mb-5">
                <button type="submit" value="affenpinscher" id="as" class="btn btn-primary btn-lg px-4 me-sm-3 mt-3" >Submit</button>
              </div>
          </form>
        </div>
        <div class="" style="max-height: 30vh;">
          <div class="container mb-5">
            <!-- If a dog breed has been chosen -->
            <% if (locals.imgUrl){ %>
                <img src="<%= imgUrl%>" class="img-fluid rounded-3 shadow-lg " width="300" height="300" loading="lazy">
           <% } %>

          </div>
        </div>
      </div>
    </div>



    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-C6RzsynM9kWDrMNeT87bh95OGNyZPhcTNXj1NW7RuBCsyN/o0jlpcV8Qyq46cDfL" crossorigin="anonymous"></script>
</body>
</html>


